<?xml version="1.0" encoding="utf-8"?><MotionScene xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:motion="http://schemas.android.com/apk/res-auto">    <Transition        motion:constraintSetEnd="@+id/end"        motion:constraintSetStart="@id/start"        motion:duration="1700">        <KeyFrameSet />        <OnSwipe motion:touchAnchorId="@+id/res" />        />    </Transition>    <ConstraintSet android:id="@+id/start">        <Constraint            android:id="@+id/res"            android:layout_width="0dp"            android:layout_height="0dp"            motion:layout_constraintBottom_toBottomOf="@+id/imageView8"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintStart_toStartOf="parent"            android:layout_marginStart="10dp"            android:layout_marginEnd="10dp"            motion:layout_constraintTop_toBottomOf="@+id/textView2" />        <Constraint            android:id="@+id/textView2"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="8dp"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintStart_toStartOf="parent"            motion:layout_constraintTop_toBottomOf="@+id/profile_picture" />        <Constraint            android:id="@+id/soihefawiw"            android:layout_width="60dp"            android:layout_height="60dp"            android:layout_marginEnd="16dp"            motion:layout_constraintBottom_toBottomOf="@+id/level"            motion:layout_constraintHorizontal_bias="1.0"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintStart_toEndOf="@+id/score"            motion:layout_constraintTop_toTopOf="@+id/name"            motion:layout_editor_absoluteY="170dp" />    </ConstraintSet>    <ConstraintSet        android:id="@+id/end"        android:layout_height="200dp">        <Constraint            android:id="@+id/soihefawiw"            android:layout_width="wrap_content"            android:layout_height="53dp"            android:layout_marginEnd="16dp"            android:layout_marginBottom="32dp"            motion:layout_constraintBottom_toBottomOf="parent"            motion:layout_constraintEnd_toEndOf="parent">            <CustomAttribute                motion:attributeName="text"                motion:customStringValue="Play" />        </Constraint>        <Constraint            android:id="@+id/textView2"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintStart_toStartOf="parent"            motion:layout_constraintTop_toTopOf="parent" />        <Constraint            android:id="@+id/res"            android:layout_width="0dp"            android:layout_height="0dp"            android:visibility="visible"            motion:layout_constraintBottom_toBottomOf="parent"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintStart_toStartOf="parent"            android:layout_marginStart="10dp"            android:layout_marginEnd="10dp"            motion:layout_constraintTop_toBottomOf="@+id/textView2" />        <Constraint            android:id="@+id/profile_picture"            motion:layout_constraintStart_toStartOf="parent"            android:visibility="gone"            motion:layout_constraintTop_toTopOf="parent" />        <Constraint            android:id="@+id/name"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="24dp"            motion:layout_constraintStart_toEndOf="@+id/profile_picture"            android:visibility="gone"            motion:layout_constraintTop_toTopOf="parent">            <CustomAttribute motion:attributeName="textSize" />        </Constraint>        <Constraint            android:id="@+id/imageView5"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            motion:layout_constraintBottom_toBottomOf="@+id/score"            motion:layout_constraintStart_toEndOf="@+id/score"            android:visibility="gone"            motion:layout_constraintTop_toTopOf="@+id/score" />        <Constraint            android:id="@+id/score"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="8dp"            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"            motion:layout_constraintStart_toEndOf="@+id/imageView4"            android:visibility="gone"            motion:layout_constraintTop_toTopOf="@+id/imageView4" />        <Constraint            android:id="@+id/level"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="8dp"            motion:layout_constraintBottom_toBottomOf="@+id/imageView5"            motion:layout_constraintStart_toEndOf="@+id/imageView5"            android:visibility="gone"            motion:layout_constraintTop_toTopOf="@+id/imageView5" />        <Constraint            android:id="@+id/imageView4"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:visibility="gone"            motion:layout_constraintStart_toEndOf="@+id/profile_picture"            motion:layout_constraintTop_toBottomOf="@+id/name" />    </ConstraintSet></MotionScene>