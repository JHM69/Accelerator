<?xml version="1.0" encoding="utf-8"?><MotionScene xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:motion="http://schemas.android.com/apk/res-auto">    <Transition        motion:constraintSetEnd="@+id/end"        motion:constraintSetStart="@id/start"        motion:duration="1000">        <KeyFrameSet />        <OnSwipe motion:touchAnchorId="@+id/refreshLayout" />    </Transition>    <ConstraintSet android:id="@+id/start">        <Constraint            android:id="@+id/soihefawiw2"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:layout_marginBottom="16dp"            motion:layout_constraintBottom_toBottomOf="parent"            motion:layout_constraintEnd_toEndOf="parent">            <CustomAttribute                motion:attributeName="text"                motion:customStringValue="Clear" />        </Constraint>    </ConstraintSet>    <ConstraintSet android:id="@+id/end">        <Constraint            android:id="@+id/soihefawiw2"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:layout_marginBottom="16dp"            motion:layout_constraintBottom_toBottomOf="parent"            motion:layout_constraintEnd_toEndOf="parent">            <CustomAttribute                motion:attributeName="text"                motion:customStringValue="Example" />        </Constraint>    </ConstraintSet></MotionScene>