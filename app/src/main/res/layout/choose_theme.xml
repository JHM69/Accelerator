<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <RadioGroup        android:id="@+id/group"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_margin="10dp"        android:layout_marginStart="32dp"        android:layout_marginTop="24dp"        android:layout_marginEnd="32dp"        android:layout_marginBottom="24dp"        android:orientation="vertical"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView8"        app:layout_constraintVertical_bias="0.0"        tools:ignore="HardcodedText">        <RadioButton            android:id="@+id/defaultTheme"            android:layout_width="match_parent"            android:layout_height="50dp"            android:foreground="?selectableItemBackgroundBorderless"            android:paddingStart="10dp"            android:text="Default"            android:textColor="@color/colorAccentt"            android:textStyle="bold"            tools:ignore="RtlSymmetry,UnusedAttribute" />        <RadioButton            android:id="@+id/darkTheme"            android:layout_width="match_parent"            android:layout_height="50dp"            android:foreground="?selectableItemBackgroundBorderless"            android:paddingStart="10dp"            android:text="Dark Theme"            android:textColor="?android:textColorPrimary"            android:textStyle="bold"            tools:ignore="RtlSymmetry,UnusedAttribute" />        <RadioButton            android:id="@+id/tealTheme"            android:layout_width="match_parent"            android:layout_height="50dp"            android:foreground="?selectableItemBackgroundBorderless"            android:paddingStart="10dp"            android:text="Sea Green"            android:textColor="#00796B"            android:textStyle="bold"            tools:ignore="HardcodedText,RtlSymmetry,UnusedAttribute" />        <RadioButton            android:id="@+id/violeteTheme"            android:layout_width="match_parent"            android:layout_height="50dp"            android:foreground="?selectableItemBackgroundBorderless"            android:paddingStart="10dp"            android:text="Light Blue"            android:textColor="#227BD3"            android:textStyle="bold"            tools:ignore="HardcodedText,RtlSymmetry,UnusedAttribute" />        <RadioButton            android:id="@+id/pinkTheme"            android:layout_width="match_parent"            android:layout_height="50dp"            android:foreground="?selectableItemBackground"            android:paddingStart="10dp"            android:text="Pastel Pink"            android:textColor="#DC828F"            android:textStyle="bold"            tools:ignore="HardcodedText,RtlSymmetry,UnusedAttribute" />        <RadioButton            android:id="@+id/lynch"            android:layout_width="match_parent"            android:layout_height="50dp"            android:foreground="?selectableItemBackground"            android:paddingStart="10dp"            android:text="Lynch"            android:textColor="#698396"            android:textStyle="bold"            android:visibility="visible"            tools:ignore="HardcodedText,RtlSymmetry,UnusedAttribute" />        <RadioButton            android:id="@+id/DelRio"            android:layout_width="match_parent"            android:layout_height="50dp"            android:foreground="?selectableItemBackgroundBorderless"            android:paddingStart="10dp"            android:text="Del Rio"            android:textColor="#897C87"            android:textStyle="bold"            tools:ignore="HardcodedText,RtlSymmetry,UnusedAttribute" />    </RadioGroup>    <TextView        android:id="@+id/textView8"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="24dp"        android:text="Choose Theme"        android:textSize="24sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:ignore="HardcodedText" /></androidx.constraintlayout.widget.ConstraintLayout>