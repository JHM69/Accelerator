<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/constraintLayout"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginTop="60dp"        android:gravity="center_vertical"        android:orientation="horizontal"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="4dp"            android:background="@drawable/gradient_1"            android:focusable="true"            android:fontFamily="@font/regular"            android:foreground="?selectableItemBackground"            android:onClick="onViewProfileClicked"            android:paddingLeft="10dp"            android:paddingRight="10dp"            android:text="@string/view_profile"            android:textColor="@color/white"            android:textSize="18sp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.0"            app:layout_constraintStart_toEndOf="@+id/profile_image"            app:layout_constraintTop_toBottomOf="@+id/username"            tools:ignore="MissingPrefix,UnusedAttribute" />        <TextView            android:id="@+id/username"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="4dp"            android:layout_marginTop="8dp"            android:fontFamily="@font/regular"            android:text="@string/username"            android:textColor="@color/colorAccentt"            android:textSize="20sp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.0"            app:layout_constraintStart_toEndOf="@+id/profile_image"            app:layout_constraintTop_toTopOf="parent" />        <de.hdodenhof.circleimageview.CircleImageView            android:id="@+id/profile_image"            android:layout_width="64dp"            android:layout_height="64dp"            android:layout_marginStart="4dp"            android:src="@drawable/ic_logo"            android:transitionName="profilePic"            app:civ_border_width="0dp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/list"        android:layout_width="240dp"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/dark_mode"        app:layout_constraintVertical_bias="0.0" />    <androidx.appcompat.widget.SwitchCompat        android:id="@+id/dark_mode"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:buttonTint="@color/colorAccentt"        android:fontFamily="@font/regular"        android:text="Dark Mode"        android:textColor="@color/colorAccentt"        android:textStyle="bold"        android:visibility="gone"        app:layout_constraintEnd_toEndOf="@+id/constraintLayout"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"        tools:ignore="HardcodedText" />    <androidx.cardview.widget.CardView        android:id="@+id/cardView"        android:layout_width="wrap_content"        android:layout_height="30dp"        android:layout_marginStart="52dp"        android:backgroundTint="?android:colorBackgroundCacheHint"        app:cardCornerRadius="15dp"        app:cardElevation="50dp"        app:layout_constraintBottom_toBottomOf="@+id/button4"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="@+id/button4">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:background="@color/colorAccentt"            android:gravity="center"            android:orientation="horizontal">            <ImageView                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_gravity="center"                android:layout_margin="5dp"                android:layout_marginTop="2dp"                android:contentDescription="TODO"                android:src="@drawable/ic_coin"                tools:ignore="ContentDescription,HardcodedText" />            <TextView                android:id="@+id/reaward"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginEnd="10dp"                android:fontFamily="@font/regular"                android:gravity="center"                android:text="500 xp"                android:textColor="@color/white"                android:textSize="19sp"                android:textStyle="bold"                tools:ignore="HardcodedText" />        </LinearLayout>    </androidx.cardview.widget.CardView>    <Button        android:id="@+id/button4"        android:layout_width="wrap_content"        android:layout_height="42dp"        android:layout_margin="0dp"        android:backgroundTint="@color/colorAccentt"        android:drawablePadding="0dp"        android:elevation="30dp"        android:fontFamily="@font/regular"        android:freezesText="true"        android:gravity="center"        android:includeFontPadding="true"        android:padding="0dp"        android:paddingLeft="10dp"        android:paddingTop="0dp"        android:paddingRight="10dp"        android:paddingBottom="0dp"        android:selectAllOnFocus="true"        android:text="   Add more 20 XP   "        android:textAlignment="center"        android:textColor="@color/white"        android:textDirection="locale"        android:textSize="14sp"        app:cornerRadius="21dp"        app:iconGravity="start"        app:iconPadding="0dp"        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@+id/cardView"        app:layout_constraintTop_toTopOf="parent"        tools:ignore="HardcodedText" />    <ImageView        android:layout_width="match_parent"        android:layout_height="match_parent"        android:alpha="0.2"        android:scaleType="fitStart"        android:src="@drawable/ic_artboard_2"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:ignore="ContentDescription" /></androidx.constraintlayout.widget.ConstraintLayout>