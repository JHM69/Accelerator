<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="?colorPrimaryDark">    <androidx.constraintlayout.motion.widget.MotionLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        app:layoutDescription="@xml/frag_home_xml_framelayout3_scene">        <LinearLayout            android:id="@+id/default_item"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:gravity="center"            android:orientation="vertical"            android:visibility="gone"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <ImageView                android:layout_width="100dp"                android:layout_height="100dp"                android:layout_marginBottom="5dp"                tools:srcCompat="@drawable/ic_logo"                tools:ignore="ContentDescription" />            <TextView                android:id="@+id/default_title"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:gravity="center"                android:text="No posts yet"                android:textColor="?android:textColorPrimary"                android:textSize="19sp"                tools:ignore="HardcodedText,MissingPrefix" />            <TextView                android:id="@+id/default_text"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="10dp"                android:layout_marginEnd="10dp"                android:gravity="center"                android:text="Add some friends to see their posts here"                tools:ignore="HardcodedText" />        </LinearLayout>        <HorizontalScrollView            android:id="@+id/ghujiko"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:scrollbars="none"            app:layout_constraintBottom_toTopOf="@+id/posts_recyclerview"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.5"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:layout_constraintVertical_chainStyle="packed">            <com.google.android.material.chip.ChipGroup                android:id="@+id/filter_chip_SS_group"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="4dp"                android:layout_marginTop="4dp"                android:layout_marginEnd="4dp"                app:singleSelection="true">                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:layout_marginStart="10dp"                    android:text="All"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="General"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="Physics"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="Chemistry"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="Math"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="Meme"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="Biology"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="GK"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="Programing"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:text="Astronomy"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />                <com.google.android.material.chip.Chip                    style="@style/Widget.MaterialComponents.Chip.Filter"                    android:layout_width="wrap_content"                    android:layout_height="50dp"                    android:layout_marginEnd="10dp"                    android:text="Motivational"                    app:checkedIconTint="#121212"                    tools:ignore="HardcodedText" />            </com.google.android.material.chip.ChipGroup>        </HorizontalScrollView>        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton            android:id="@+id/soihefawiw"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:layout_marginBottom="16dp"            android:backgroundTint="@color/colorSec"            android:clickable="true"            android:fontFamily="@font/regular"            android:src="@drawable/ic_add_24dp"            android:text="Post"            android:textColor="@color/white"            app:icon="@drawable/ic_add_24dp"            app:iconTint="@color/white"            app:layout_behavior="com.google.android.material.behavior.HideBottomViewOnScrollBehavior"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            tools:ignore="HardcodedText,KeyboardInaccessibleWidget" />        <androidx.recyclerview.widget.RecyclerView            android:id="@+id/posts_recyclerview"            android:layout_width="0dp"            android:layout_height="0dp"            android:layout_marginTop="4dp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.5"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/ghujiko" />    </androidx.constraintlayout.motion.widget.MotionLayout></FrameLayout>